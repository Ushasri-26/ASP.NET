@model IEnumerable<MVC_Application.Models.Customer>
@{
    ViewBag.Title = "CustomersFromGermany";
}

<h2>Customers From Germany</h2>
<table border="1">
    <tr>
        <th>Customer ID</th>
        <th>Company Name</th>
        <th>City</th>
        <th>Country</th>
    </tr>
@foreach (var item in Model)
            {
                <tr>
                    <td>@item.CustomerID</td>
                    <td>@item.CompanyName</td>
                    <td>@item.City</td>
                    <td>@item.Country</td>
                </tr>
            }
</table>

